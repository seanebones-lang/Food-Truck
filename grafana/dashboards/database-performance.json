{
  "dashboard": {
    "title": "Database Performance Dashboard",
    "tags": ["database", "performance"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "Query Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(database_queries_total[5m])",
            "legendFormat": "Queries/sec"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Slow Queries",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(database_queries_slow_total[5m])",
            "legendFormat": "Slow Queries/sec"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "Slow Query Percentage",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(database_queries_slow_total[5m]) / rate(database_queries_total[5m]) * 100",
            "legendFormat": "Slow Query %"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      }
    ]
  }
}
